PROCEDURE P_MUEVE
DO CASE
CASE M.MOVIMIENTO = 1
	GO TOP
	WAIT WINDOW "PRINCIPIO DEL ARCHIVO" NOWAIT
CASE M.MOVIMIENTO = 4
	if eof()
		WAIT WINDOW "PRINCIPIO DE ARCHIVO" NOWAIT
		GO TOP
	else
		skip
		IF eof()
			WAIT WINDOW "PRINCIPIO DE ARCHIVO" NOWAIT
			GO TOP
		ENDIF
	endif
CASE M.MOVIMIENTO = 3
	BROWSE NOMODIFY NOAPPEND NODELETE
	show gets
CASE M.MOVIMIENTO = 2
	if .not. bof()
		SKIP - 1 
		IF bof()
			WAIT WINDOW "FIN DE ARCHIVO" NOWAIT
			GO BOTTOM
		ENDIF
	else
		WAIT WINDOW "FIN DE ARCHIVO" NOWAIT
		GO BOTTOM
	ENDIF
CASE M.MOVIMIENTO = 5
	GO BOTTOM
	WAIT WINDOW "FIN DEL ARCHIVO " NOWAIT
ENDCASE
SCATTER MEMVAR
SHOW GETS
RETURN